{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--env-file=.env"
            ],
            "program": "${workspaceFolder}/index.js",
            "envFile": "${workspaceFolder}/user.dev.env",
        },
    ]
}