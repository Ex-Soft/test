<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-access="property">
	<class name="BiDirectional.BLeg, NHibernateOneToMany" table="leg">
		<id name="Id">
	      <generator class="native" />
	    </id>
		<property name="Position"/>
		<many-to-one name="Owner" class="BiDirectional.BDog, NHibernateOneToMany" column="DogId"/>
	</class>
</hibernate-mapping>

